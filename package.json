{
  "name": "node-tl1-fiberhome",
  "version": "1.0.1",
  "description": "integration module for fiberhome TL1 communication interface",
  "main": "./dist/app.js",
  "types": "./dist/app.d.ts",
  "scripts": {
    "prepare": "./node_modules/.bin/pegjs src/operationalResponseParser.pegjs && ./node_modules/.bin/pegjs src/queryResponseParser.pegjs",
    "build": "rm -rf ./dist && tslint --project . && tsc",
    "postbuild": "./node_modules/.bin/pegjs -o dist/operationalResponseParser.js src/operationalResponseParser.pegjs && ./node_modules/.bin/pegjs -o dist/queryResponseParser.js src/queryResponseParser.pegjs",
    "prepublishOnly": "rm -rf ./dist && tslint --project . && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/felipebergamin/node-tl1-fiberhome.git"
  },
  "keywords": [
    "node",
    "tl1",
    "fiberhome"
  ],
  "author": "Felipe Bergamin <felipebergamin6@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/felipebergamin/node-tl1-fiberhome/issues"
  },
  "homepage": "https://github.com/felipebergamin/node-tl1-fiberhome#readme",
  "devDependencies": {
    "@types/node": "^8.5.2",
    "pegjs": "^0.10.0"
  },
  "dependencies": {
    "@types/debug": "0.0.30",
    "debug": "^3.1.0",
    "rxjs": "^6.2.2"
  }
}
